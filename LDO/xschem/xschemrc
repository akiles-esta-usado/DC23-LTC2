append XSCHEM_LIBRARY_PATH :$script_path/bandgap
append XSCHEM_LIBRARY_PATH :$script_path/ldo
append XSCHEM_LIBRARY_PATH :$script_path/ldo-top

set XSCHEM_START_WINDOW {0_top.sch}
